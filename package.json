{
  "name": "res-api",
  "version": "1.0.0",
  "description": "El servidor backend del sitema de notas. Esta documentación explica cómo configurar, iniciar y probar el servidor backend.\r El proyecto esta realizado en:",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "seed:generate": "sequelize-cli seed:generate --name",
    "seed:run": "sequelize-cli db:seed:all",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "seed:revert": "sequelize-cli db:seed:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.2",
    "sequelize-cli": "^6.4.1"
  },
  "engines": {
    "node": "14.x"
  },
  "devDependencies": {
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.0",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1"
  }
}
